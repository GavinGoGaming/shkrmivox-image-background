{
  "manifest_version": 2,
  "version": "2.4.2",
  "name": "Shkrmivox and Images",
  "description": "Skiovox on a new level.",
  "icons": {
    "128": "icon.png"
  },
  "commands": {
    "ACCESS_HISTORY": {
      "description": "Ctrl+H",
      "suggested_key": {
        "default": "Ctrl+H"
      }
    },
    "ACCESS_DOWNLOADS": {
      "description": "Ctrl+J",
      "suggested_key": {
        "default": "Ctrl+J"
      }
    },
    "NEW_TAB": {
      "description": "Ctrl+T",
      "suggested_key": {
        "default": "Ctrl+T"
      }
    },
    "CLOSE_TAB": {
      "description": "Ctrl+W",
      "suggested_key": {
        "default": "Ctrl+W"
      }
    },
    "RESTORE_TAB": {
      "description": "Ctrl+Shift+T",
      "suggested_key": {
        "default": "Ctrl+Shift+T"
      }
    },
    "NEW_WINDOW": {
      "description": "Ctrl+N",
      "suggested_key": {
        "default": "Ctrl+N"
      }
    },
    "NEW_INCOG_WINDOW": {
      "description": "Ctrl+Shift+N",
      "suggested_key": {
        "default": "Ctrl+Shift+N"
      }
    },
    "CLOSE_WINDOW": {
      "description": "Ctrl+Shift+W",
      "suggested_key": {
        "default": "Ctrl+Shift+W"
      }
    },
    "TAB_NEXT": {
      "description": "Cycle forwards through tabs: Ctrl+Q",
      "suggested_key": {
        "default": "Ctrl+Q"
      }
    },
    "TAB_BACK": {
      "description": "Cycle backwards through tabs: Ctrl+Shift+Q",
      "suggested_key": {
        "default": "Ctrl+Shift+Q"
      }
    },
    "SWITCH_WINDOWS": {
      "description": "Switch windows: Alt+Q",
      "suggested_key": {
        "default": "Alt+Q"
      }
    },
    "CTRL_1": {
      "description": "Ctrl+1",
      "suggested_key": {
        "default": "Ctrl+1"
      }
    },
    "CTRL_2": {
      "description": "Ctrl+2",
      "suggested_key": {
        "default": "Ctrl+2"
      }
    },
    "CTRL_3": {
      "description": "Ctrl+3",
      "suggested_key": {
        "default": "Ctrl+3"
      }
    },
    "CTRL_4": {
      "description": "Ctrl+4",
      "suggested_key": {
        "default": "Ctrl+4"
      }
    },
    "CTRL_5": {
      "description": "Ctrl+5",
      "suggested_key": {
        "default": "Ctrl+5"
      }
    },
    "CTRL_6": {
      "description": "Ctrl+6",
      "suggested_key": {
        "default": "Ctrl+6"
      }
    },
    "CTRL_7": {
      "description": "Ctrl+7",
      "suggested_key": {
        "default": "Ctrl+7"
      }
    },
    "CTRL_8": {
      "description": "Ctrl+8",
      "suggested_key": {
        "default": "Ctrl+8"
      }
    },
    "CTRL_9": {
      "description": "Ctrl+9",
      "suggested_key": {
        "default": "Ctrl+9"
      }
    },
    "VIEW_SOURCE": {
      "description": "Ctrl+U",
      "suggested_key": {
        "default": "Ctrl+U"
      }
    }
  },
  "background": {
    "scripts": [
      "/background/commands.js",
      "/background/setup.js",
      "/background/webstore-fix.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "/new-tab/main.html"
  },
  "permissions": [
    "commands",
    "tabs",
    "sessions",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "/content-scripts/window-open-fix.js"
      ],
      "all-frames": true,
      "run-at": "document_start"
    }
  ]
}
